{
  "name": "yorki/laravel-workshop",
  "description": "Laravel workshop",
  "license": "MIT",
  "keywords": [
    "laravel",
    "workshop"
  ],
  "authors": [
    {
      "name": "Norbert Pabisz",
      "email": "yorki.ogame@gmail.com"
    }
  ],
  "homepage": "https://github.com/Yorkii/laravel-workshop",
  "require": {
    "php": ">=7.0",
    "illuminate/support": ">=5.5",
    "illuminate/contracts": ">=5.5",
    "illuminate/view": ">=5.5",
    "illuminate/container": ">=5.5",
    "illuminate/console": ">=5.5",
    "illuminate/cache": ">=5.5",
    "arrilot/laravel-widgets": "^3.12",
    "yorki/model-repository": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Yorki\\Workshop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yorki\\Workshop\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yorki\\Workshop\\ServiceProvider"
      ]
    }
  }
}